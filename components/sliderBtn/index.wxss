.content{
  margin: 0 auto;
  height: 90rpx;
  background: rgba(215,215,215,0.8);
  color: #666;
  border-radius: 25px;
  display:flex;
  justify-content:center;
  align-items:center;
  font-size:16px;
  overflow: hidden;
}
.box{
  height: 90rpx;
  background-color: rgba(246,119,120,1);
  border-radius: 25px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.movable-icon{
  width: 60rpx;
  height: 60rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100% 100%;
}
.movable-icon-success{
  width: 60rpx;
  height: 60rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100% 100%;
}